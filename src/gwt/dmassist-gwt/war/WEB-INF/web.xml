<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
    "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>
  
  <!-- Servlets -->
  <servlet>
    <servlet-name>greetServlet</servlet-name>
    <servlet-class>com.ziroby.dmassist.gwt.server.GreetingServiceImpl</servlet-class>
  </servlet>
  
  <servlet>
    <servlet-name>initListServlet</servlet-name>
    <servlet-class>com.ziroby.dmassist.gwt.server.InitListServiceImpl</servlet-class>
  </servlet>
  
  
  <servlet-mapping>
    <servlet-name>greetServlet</servlet-name>
    <url-pattern>/dmassist_gwt/greet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>initListServlet</servlet-name>
    <url-pattern>/dmassist_gwt/initList</url-pattern>
  </servlet-mapping>
  
  <!-- Default page to serve -->
  <welcome-file-list>
    <welcome-file>Dmassist_gwt.html</welcome-file>
  </welcome-file-list>

</web-app>
